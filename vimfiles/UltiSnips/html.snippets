snippet com "comment" i
<!-- $0 -->
endsnippet

snippet i "an inline tag"
<${1:span}>$0</${1/(\w+).*/$1/}>
endsnippet

snippet b "a block tag" b
<${1:div}>
    $0
</${1/(\w+).*/$1/}>
endsnippet

snippet scr "script" b
<script type="text/javascript">
$(function(){
    $0
});
</script>
endsnippet

snippet sty "style" b
<style>
$1 {
    $0
}
</style>
endsnippet

